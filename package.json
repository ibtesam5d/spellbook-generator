		"@typescript-eslint/parser": "^5.45.0",
		"autoprefixer": "^10.4.13",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"postcss": "^8.4.20",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.8.1",
		"prisma": "^4.12.0",
		"svelte": "^3.54.0",
		"svelte-check": "^2.9.2",
		"tailwind-scrollbar": "^3.0.0",
		"tailwindcss": "^3.2.4",
